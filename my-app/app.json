{
  "expo": {
    "name": "my-app",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bigbasket-logo.png",
    "splash": {
      "image": "./assets/images/bigbasket-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bigbasket-logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "androidNavigationBar": {
      "visible": "sticky",
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/bigbasket-logo.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
